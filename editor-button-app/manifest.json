{
  "identifier": "string-key-extractor",
  "name": "String Key Extractor",
  "description": "Extract current string keys and context from Crowdin editor",
  "baseUrl": "https://editor-button-app.vercel.app",
  "authentication": {
    "type": "none"
  },
  "scopes": ["project"],
  "modules": {
    "editor-right-panel": [
      {
        "key": "string-key-panel",

{
  "identifier": "editor-button-app",
  "name": "Editor Button App",
  "description": "Adds a custom button to the Crowdin editor window",
  "baseUrl": "https://editor-button-app.vercel.app",
  "authentication": {
    "type": "none"
  },
  "scopes": ["project"],
  "modules": {
    "editor-right-panel": [
      {

        "key": "editor-button",
        "name": "Custom Editor Button",
        "modes": ["translate", "comfortable", "side-by-side", "multilingual"],
        "url": "/editor-button",
        "environments": ["crowdin"]
      }
    ]
  }
}
